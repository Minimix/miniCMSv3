<div class="mbm-panel grid_8">
    <div class="mbm-panel-header">
        <span class="mbm-icon icon-24-sign-post">Register New Member</span>
    </div>
    <div class="mbm-panel-body">
        <div class="mbm-wizard clearfix">
            <ul>
                <li>
                    <a class="mbm-icon icon-16-user" href="#">Member Profile</a>
                </li>
                <li class="current">
                    <a href="#" class="mbm-icon icon-16-delivery">Membership type</a>
                </li>
                <li>
                    <a class="mbm-icon icon-16-accept" href="#">Confirmation</a>
                </li>
            </ul>
        </div>
        <form class="mbm-form" action="http://www.malijuwebshop.com/themes/mbm-admin/dashboard.html">
            <div class="mbm-form-inline">
                <div class="mbm-form-row">
                    <label>Fullname</label>
                    <div class="mbm-form-item large">
                        <input type="text" name="fullname" class="mbm-textinput" />
                    </div>
                </div>
                <div class="mbm-form-row">
                    <label>Email</label>
                    <div class="mbm-form-item large">
                        <input type="text" name="email" class="mbm-textinput" />
                    </div>
                </div>
                <div class="mbm-form-row">
                    <label>Address</label>
                    <div class="mbm-form-item large">
                        <textarea name="address" rows="100%" cols="100%"></textarea>
                    </div>
                </div>
                <div class="mbm-form-row">
                    <label>Gender</label>
                    <div class="mbm-form-item">
                        <ul class="mbm-form-list">
                            <li><input type="radio" id="male" name="gender" /> <label for="male">Male</label></li>
                            <li><input type="radio" id="female" name="gender" /> <label for="female">Female</label></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="mbm-button-row">
                <input type="submit" value="Prev" class="mbm-button gray left" />
                <input type="submit" value="Next" class="mbm-button green" />
            </div>
        </form>
    </div>
</div>

